{
    "name": "Shoto",
    "description": "Notion search from URL bar",
    "version": "1.1.0",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "omnibox": {
        "keyword": "/"
    },
    "options_page": "options.html",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "16": "assets/shoto16.png",
            "32": "assets/shoto32.png"
        }
    },
    "permissions": [
        "activeTab",
        "storage",
        "*://www.notion.so/*"
    ],
    "icons": {
        "16": "assets/shoto16.png",
        "32": "assets/shoto32.png",
        "48": "assets/shoto48.png",
        "128": "assets/shoto128.png"
    }
}